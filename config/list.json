{
		"General": [
			{ "name": "help", "args": ["(Optional name)"], "aliases": [], "desc": "DMs the author the help page" },
			{ "name": "prefix", "args": ["(Optional value)"], "aliases": [], "desc": "Gets or sets the server's prefix" },
			{ "name": "get", "args": ["clanID"], "aliases": [], "desc": "Sends a page with details about a clan" },
			{ "name": "list", "args": [], "aliases": ["view"], "desc": "A list of all clans in a server" },
			{ "name": "join", "args": ["clanID"], "aliases": [], "desc": "Joins a clan if it's public" },
			{ "name": "leave", "args": ["clanID"], "aliases": [], "desc": "Leaves a clan" },
			{ "name": "invite", "args": ["user", "clanID"], "aliases": [], "desc": "Invites a user to a clan" },
			{ "name": "icon", "args": ["clanID"], "aliases": [], "desc": "Sends a clan's icon" },
			{ "name": "banner", "args": ["clanID"], "aliases": [], "desc": "Sends a clan's banner" }
		],
  
		"Management": [
			{ "name": "create", "args": ["(Optional name)"], "aliases": [], "desc": "Creates a new clan" },
			{ "name": "set", "args": ["attr", "clanID", "value"], "aliases": ["edit"], "desc": "Sets an attribute in a clan" },
			{ "name": "shout", "args": ["clanID"], "aliases": [], "desc": "Changes the clan shout" },
			{ "name": "disband", "args": ["clanID"], "aliases": [], "desc": "Deletes a clan" },
			{ "name": "allies", "args": ["clanID", "add/remove", "clanIDs"], "aliases": ["alliances"], "desc": "Editor for a clan's allies" },
			{ "name": "enemies", "args": ["clanID", "add/remove", "clanIDs"], "aliases": ["rivalries", "rivals"], "desc": "Editor for a clan's enemies" }
		],
  
		"Moderation": [
			{ "name": "ban", "args": ["user", "clanID"], "aliases": [], "desc": "Bans a user from a clan" },
			{ "name": "unban", "args": ["user", "clanID"], "aliases": [], "desc": "Unbans a user from a clan" },
			{ "name": "kick", "args": ["user", "clanID"], "aliases": [], "desc": "Kicks a user from a clan" },
			{ "name": "op", "args": ["user", "clanID"], "aliases": [], "desc": "Gives a user operator permissions" },
			{ "name": "deop", "args": ["user", "clanID"], "aliases": [], "desc": "Removes a users operator permissions" }
		],
  
		"Economy": [
			{ "name": "balance", "args": ["(Optional user)"], "aliases": ["bal"], "desc": "Sends someone's pearl balance" },
			{ "name": "work", "args": [], "aliases": [], "desc": "Lets you gain pearls" },
			{ "name": "deposit", "args": ["amount", "clanID"], "aliases": ["dep"], "desc": "Deposits pearls into a clan" },
			{ "name": "withdraw", "args": ["amount", "clanID"], "aliases": [], "desc": "Removes pearls from a clan YOU OWN" },
			{ "name": "give", "args": ["user", "amount"], "aliases": [], "desc": "Gives a user pearls" },
			{ "name": "steal", "args": ["user"], "aliases": [], "desc": "Steals pearls from a user" },
			{ "name": "slots", "args": ["bet"], "aliases": [], "desc": "A slot machine that can multiply your bet by up to 10x" },
			{ "name": "shop", "args": [], "aliases": ["store"], "desc": "Sends the shop page for the server" },
			{ "name": "buy", "args": ["item", "(Optional amount)"], "aliases": [], "desc": "Buys an item for pearls" },
			{ "name": "sell", "args": ["item", "(Optional amount))"], "aliases": [], "desc": "Sells an item for pearls" },
			{ "name": "use", "args": ["item"], "aliases": [], "desc": "Uses a bought item" }
		],
		
		"Administrator": [
			{ "name": "export", "args": [], "aliases": [], "desc": "Sends a JSON file with all clans in a server" },
			{ "name": "import", "args": ["file"], "aliases": [], "desc": "Replaces all clans in a server with those from a JSON file" },
			{ "name": "listall", "args": [], "aliases": [], "desc": "Gets a list of all clans including unlisted ones" },
			{ "name": "forcedisband", "args": ["clanID"], "aliases": ["forcedb", "fdb", "fdisband"], "desc": "Disbands a clan by force" },
			{ "name": "forceset", "args": ["attr", "clanID", "value"], "aliases": ["fset"], "desc": "Sets a thing in a clan by force" },
			{ "name": "forceshout", "args": ["clanID", "content"], "aliases": ["fshout"], "desc": "Sets a clan shout by force" },
			{ "name": "forcejoin", "args": ["clanID"], "aliases": ["fjoin"], "desc": "Joins a clan even if it's private" },
			{ "name": "forceban", "args": ["user", "clanID"], "aliases": ["fban"], "desc": "Bans a user by force" },
			{ "name": "forceunban", "args": ["user", "clanID"], "aliases": ["funban"], "desc": "Unbans a user by force" },
			{ "name": "forcekick", "args": ["user", "clanID"], "aliases": ["fkick"], "desc": "Kicks a user by force" },
			{ "name": "addfunds", "args": ["amount", "clanID"], "aliases": ["fadd"], "desc": "Adds funds to a clan" },
			{ "name": "remfunds", "args": ["amount", "clanID"], "aliases": ["fremove"], "desc": "Removes funds from a clan" },
			{ "name": "setfunds", "args": ["amount", "clanID"], "aliases": ["fset"], "desc": "Sets funds in a clan" },
			{ "name": "clearfunds", "args": ["amount", "clanID"], "aliases": ["fclear"], "desc": "Clears funds for a clan" }
		]
}
